    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.2",
    "esbuild": "^0.17.12",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "typescript": ">=5"
  }
